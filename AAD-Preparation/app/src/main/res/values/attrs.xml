<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BottomSheetItem">
        <attr name="c_title" format="string" />
        <attr name="c_icon" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="Slider">
        <attr name="c_name" format="string" />
        <attr name="c_progress_desc" format="string" />
        <attr name="c_min_progress" format="integer" />
        <attr name="c_max_progress" format="integer" />
        <attr name="c_progress" format="integer" />
    </declare-styleable>

    <declare-styleable name="StepItem">
        <attr name="s_title" format="string" />
        <attr name="s_is_first_step" format="boolean" />
        <attr name="s_is_last_step" format="boolean" />
        <attr name="s_is_done" format="boolean" />
    </declare-styleable>
</resources>